package io.dddrive.core.enums.model.base

import io.dddrive.core.enums.model.Enumerated
import io.dddrive.core.enums.model.Enumeration

abstract class EnumeratedBase(
	override val enumeration: Enumeration<out Enumerated>,
	override val id: String,
	private val itemName: String,
) : Enumerated {

	override fun name() = itemName

	override fun toString(): String = this.javaClass.getSimpleName() + "[" + this.id + "]"

}

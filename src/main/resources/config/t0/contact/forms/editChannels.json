{
	"id": "contact/editChannels",
	"formDef": {
		"rows": [
			{
				"cols": [
					{
						"type": "subform",
						"size": 12,
						"label": "Interaction Channels",
						"isRequired": false,
						"ignore": false,
						"enabled": true,
						"layoutDefinition": {
							"rows": [
								{
									"cols": [
										{
											"type": "text",
											"label": "Type",
											"size": 3,
											"isRequired": false,
											"enabled": true,
											"value": "{{channelTypeId}}"
										},
										{
											"type": "text",
											"label": "Channel",
											"size": 3,
											"isRequired": false,
											"enabled": true,
											"value": "{{channelId}}"
										},
										{
											"type": "text",
											"label": "Contact",
											"size": 4,
											"isRequired": false,
											"enabled": true,
											"value": "{{name}}"
										},
										{
											"type": "boolean",
											"label": "Favorite",
											"size": 2,
											"isRequired": false,
											"enabled": true,
											"value": "{{favorite}}"
										}
									]
								}
							]
						},
						"multipleEntries": true,
						"value": "{{contact.interactionChannels}}"
					}
				]
			},
			{
				"cols": [
					{
						"type": "accordion",
						"visible": "{{!control.enabled}}",
						"sections": [
							{
								"type": "panel",
								"label": "System Information",
								"rows": [
									{
										"cols": [
											{
												"size": 6,
												"type": "text",
												"label": "Created By",
												"value": "{{contact.meta.createdByUser.caption}}",
												"visible": "{{!control.enabled}}",
												"enabled": false
											},
											{
												"size": 6,
												"type": "date",
												"enableTime": true,
												"format": "dddd, DD.MM.YYYY HH:mm",
												"label": "&nbsp;",
												"value": "{{contact.meta.createdAt}}",
												"visible": "{{!control.enabled}}",
												"enabled": false
											},
											{
												"size": 6,
												"type": "text",
												"label": "Last Modified By",
												"value": "{{contact.meta.modifiedByUser.caption}}",
												"visible": "{{!control.enabled}}",
												"enabled": false
											},
											{
												"size": 6,
												"type": "date",
												"enableTime": true,
												"format": "dddd, DD.MM.YYYY HH:mm",
												"label": "&nbsp;",
												"value": "{{contact.meta.modifiedAt}}",
												"visible": "{{!control.enabled}}",
												"enabled": false
											}
										]
									}
								]
							}
						]
					}
				]
			}
		]
	}
}
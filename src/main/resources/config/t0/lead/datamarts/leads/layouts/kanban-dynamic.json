{
	"id": "kanban-dynamic",
	"name": "Kanban",
	"layout": {
		"layoutType": "kanban",
		"isTimelineHeader": true,
		"header": {
			"dynamic": {
				"url": "{{enumBaseUrl}}/doc/codeCaseStage/lead",
				"valueField": "id",
				"displayNameField": "name"
			}
		},
		"data": [
			"{{data}}",
			{
				"id": "{{id}}",
				"link": "/lead/{{id}}",
				"stageId": "{{caseStage.id}}",
				"stageName": "{{caseStage.name}}",
				"topic": "{{subject}}",
				"name": "=> nvl(name, contact.caption)",
				"source": "{{leadSource.name}}",
				"remarks": "{{remarks}}"
			}
		],
		"groupBy": {
			"value": "stageId",
			"serverFieldName": "caseStage"
		},
		"cardLayout": {
			"title": {
				"value": "topic",
				"displayName": "topic"
			},
			"body": [
				{
					"value": "name",
					"displayName": "name"
				},
				{
					"value": "source",
					"displayName": "source"
				},
				{
					"value": "remarks",
					"displayName": "remarks"
				}
			]
		},
		"docType": "lead",
		"modifyUrl": "{{apiBaseUrl}}/lead/leads"
	}
}